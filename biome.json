{
  "$schema": "https://biomejs.dev/schemas/1.8.2/schema.json",
  "organizeImports": {
    "enabled": false
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "style": {
        "useImportType": "off",
        "useEnumInitializers": "off",
        "useNumberNamespace": "off"
      },
      "complexity": {
        "recommended": true
      }
    }
  }
}
